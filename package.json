{
  "name": "admin-panel",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run version && vue-tsc && vite build",
    "build:prod": "npm run version && vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "vue-i18n-extract": "vue-i18n-extract report --vueFiles './src/**/*.?(js|vue|ts)' --languageFiles './src/i18n/locales/*.?(json|yml|yaml|js)'",
    "version": "node scripts/generate-version.js",
    "version:check": "node scripts/generate-version.js --check"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "@tailwindcss/vite": "^4.1.10",
    "axios": "^1.9.0",
    "pinia": "^3.0.3",
    "qrcode": "^1.5.4",
    "tailwindcss": "^4.1.10",
    "vue": "^3.5.16",
    "vue-i18n": "^11.1.5",
    "vue-i18n-extract": "^2.0.7",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.1",
    "@types/qrcode": "^1.5.5",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/eslint-plugin": "^1.2.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.28.0",
    "eslint-plugin-oxlint": "^1.0.0",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.0.0",
    "prettier": "3.5.3",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vitest": "^3.2.3",
    "vue-tsc": "^2.2.10"
  },
  "packageManager": "yarn@4.9.2"
}
